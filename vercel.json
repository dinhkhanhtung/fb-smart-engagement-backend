{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/admin",
            "dest": "server.js"
        },
        {
            "src": "/test",
            "dest": "server.js"
        },
    {
      "src": "/test-html",
      "dest": "server.js"
    },
    {
      "src": "/payment",
      "dest": "server.js"
    },
        {
            "src": "/",
            "dest": "server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}